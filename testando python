print(lista)
for nome in lista:
     print(nome)


numeros = [10,20,30,50,60,80]    
print("Soma = ", sum (numeros))
print("Total de numeros = ", len(numeros))
print("Menor valor = ", min(numeros))
print("maior valor = ", max(numeros))    
